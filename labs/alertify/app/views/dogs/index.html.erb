<input type = "button" id="b1" value="Alert" class="alertify-button" />
<input type = "button" id="b2" value="Log" class="alertify-button"/>
<input type = "button" id="b3" value="Success" class="alertify-button"/>
<input type = "button" id="b4" value="Error" class="alertify-button"/>

<h1>Listing dogs</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dogs.each do |dog| %>
  <tr>
    <td><%= dog.name %></td>
    <td><%= link_to 'Show', dog %></td>
    <td><%= link_to 'Edit', edit_dog_path(dog) %></td>
    <td><%= link_to 'Destroy', dog, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

